// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;


int main() {
    
    int n;
    cin>>n;
    
    bool isprime = true;
    
    if(n<2){
        return false;
    }else{
        for(int i=2;i<n/2;i++){
            if(n%i==0){
                isprime=false;
                break;
            }
        }
    }
    
    if(isprime){
        cout<<"Yes";
    }else{
    cout<<"no";
    }
    return 0;
}
