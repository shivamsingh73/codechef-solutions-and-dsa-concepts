// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int main() {
  int a=5,b=9;
  
  cout<<"a & b = "<<(a&b)<<endl;
  
  cout<<" a | b = "<<(a|b)<<endl;
  
  cout<<" a^b="<< (a^b)<<endl;   //xor if two bits different
  
  cout << "~a = " << (~a) << endl;   //bitwise not -- inverts all number
  
   cout<<"b << 1" <<" = "<< (b << 1) <<endl;   //left shift
   
   cout<<"b >> 1 "<<"= " << (b >> 1 )<<endl;  //right shift
   
   // The left-shift and right-shift operators should not be used for negative numbers.
   
   
    return 0;
}
